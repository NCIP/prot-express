<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
    "http://struts.apache.org/dtds/struts-2.0.dtd">

<struts>
    <constant name="struts.enable.SlashesInActionNames" value="true" />
    <package name="default" extends="struts-default">
        <action name="protocol/list" class="gov.nih.nci.protexpress.ui.actions.protocol.ProtocolManagementAction"
            method="list">
            <result name="success">/WEB-INF/jsp/protocols.jsp</result>
        </action>

        <action name="protocol/save" class="gov.nih.nci.protexpress.ui.actions.protocol.ProtocolManagementAction"
            method="saveOrUpdate">
            <result name="success" type="redirect-action">protocol/list</result>
            <result name="input">/WEB-INF/jsp/protocolForm.jsp</result>
            <interceptor-ref name="paramsPrepareParamsStack" />
        </action>
    </package>
</struts>
<settings>
  <profiles>
    <profile>
      <id>bda</id>
      <properties>
        <schema.file>@db.protexpress.file.name@</schema.file>
        <hibernate.drop>@db.protexpress.generate.drop@</hibernate.drop>
        <hibernate.create>@db.protexpress.generate.create@</hibernate.create>
        <jdbc.url>jdbc:postgresql://DB_SERVER_NAME:DB_PORT_NUMBER/DB_NAME</jdbc.url>
        <jdbc.username>DB_USERNAME</jdbc.username>
        <jdbc.password>DB_USER_PASSWORD</jdbc.password>
        <ldap.install>LDAP_INSTALL</ldap.install>
        <ldap.host.url>LDAP_HOSTNAME</ldap.host.url>
        <ldap.searchbase>LDAP_SEARCHBASE</ldap.searchbase>
        <ldap.searchprefix>LDAP_SEARCHPREFIX</ldap.searchprefix>
        <build.svn.url>@build.svn.url@</build.svn.url>
        <build.tag>@build.tag@</build.tag>
        <build.time>@build.time@</build.time>
        <build.node>INSTALLATION_LOCATION</build.node>
        <upt.url>UPT_URL</upt.url>
      </properties>
    </profile>
  </profiles>
  <activeProfiles>
    <activeProfile>bda</activeProfile>
  </activeProfiles>
</settings>

<?xml version="1.0" encoding="ISO-8859-1"?>

<decorators defaultdir="/decorators">
    <excludes>
        <pattern>/stylesheets/*</pattern>
        <pattern>/scripts/*</pattern>
        <pattern>/images/*</pattern>
    </excludes>

	<decorator name="protocolMain" page="/WEB-INF/decorators/protocolMain.jsp">
        <pattern>/protocol/*</pattern>
        <param name="page.name" value="/WEB-INF/decorators/main.jsp" />
    </decorator>
    
    <decorator name="experimentMain" page="/WEB-INF/decorators/experimentMain.jsp">
        <pattern>/experiment/*</pattern>
        <param name="page.name" value="/WEB-INF/decorators/main.jsp" />
    </decorator>
    
    <decorator name="main" page="/WEB-INF/decorators/main.jsp">
        <pattern>/*</pattern>
        <param name="page.name" value="/WEB-INF/decorators/main.jsp" />
    </decorator>
</decorators>